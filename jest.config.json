{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".(spec|test|e2e-spec).ts$",
  "moduleNameMapper": {
    "^src/(.+)$": "<rootDir>/src/$1"
  },
  "moduleDirectories": [
   
    "node_modules"
    
],

  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
